{
  "name": "work-manager-e2e-test",
  "version": "1.0.0",
  "description": "Work Manager Automation Tests",
  "main": "conf.js",
  "scripts": {
    "tsc": "tsc",
    "clean": "rimraf temp/ && rimraf download/ && rimraf test-results/",
    "webdriver-update": "node node_modules/protractor/bin/webdriver-manager update",
    "pretest": "npm run webdriver-update && npm run clean && npm run tsc",
    "lint": "npx tslint --project . --fix",
    "test": "protractor temp/conf.js",
    "test:local": "cp config/wm-automation-config-local.json config/config.json && protractor temp/conf.js",
    "test:dev": "cp config/wm-automation-config-dev.json config/config.json && protractor temp/conf.js",
    "test:prod": "cp config/wm-automation-config-prod.json config/config.json && protractor temp/conf.js"
  },
  "keywords": [
    "protractor",
    "jasmine",
    "typescript"
  ],
  "dependencies": {
    "jasmine-reporters": "^2.4.0",
    "moment": "^2.29.1",
    "protractor": "^7.0.0",
    "protractor-beautiful-reporter": "^1.3.6",
    "topcoder-testing-lib": "git+https://github.com/topcoder-platform/topcoder-testing-lib.git#develop",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.10",
    "webdriver-manager": "^12.1.8",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/jasmine": "^3.8.2",
    "tslint": "^6.1.3"
  }
}